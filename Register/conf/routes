GET     /                       controllers.Application.index()
GET     /welcome                controllers.Application.welcome()

GET     /loginForm              controllers.Users.loginForm()
GET     /registerForm           controllers.Users.registerForm()
GET     /addBookForm           controllers.Users.addBookForm()
GET     /createRecommendationForm   controllers.Users.createRecommendationForm()
GET     /userInfo               controllers.Users.userInfo()
POST     /login                  controllers.Users.login()
POST	/register               controllers.Users.register()
POST    /addBook                controllers.Users.addBook()
POST    /createRecommendation                controllers.Users.createRecommendation()

GET     /assets/*file               controllers.Assets.at(path="/public", file)